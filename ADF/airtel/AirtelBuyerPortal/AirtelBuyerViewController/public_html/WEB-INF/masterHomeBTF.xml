<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="masterHomeBTF">
    <default-activity>fetchWKFVendorSiteRequest</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>masterHomeBean</managed-bean-name>
      <managed-bean-class>com.airtel.buyer.bean.buyerhome.MasterHomeBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="pendingRequests">
      <page>/pageFragments/masterHome/pendingRequests.jsff</page>
    </view>
    <view id="vendorResponses">
      <page>/pageFragments/masterHome/vendorResponses.jsff</page>
    </view>
    <view id="foreignVendorPendingCAApproval">
      <page>/pageFragments/masterHome/foreignVendorPendingCAApproval.jsff</page>
    </view>
    <view id="vendorResponsesToCARejection">
      <page>/pageFragments/masterHome/vendorResponsesToCARejection.jsff</page>
    </view>
    <view id="viewAttachments">
      <page>/pageFragments/masterHome/viewAttachments.jsff</page>
    </view>
    <view id="vendorResponseDetail">
      <page>/pageFragments/masterHome/vendorResponseDetail.jsff</page>
    </view>
    <view id="rejectionTitle">
      <page>/pageFragments/masterHome/rejectionTitle.jsff</page>
    </view>
    <view id="vendorResponseDetail1">
      <page>/pageFragments/masterHome/vendorResponseDetail.jsff</page>
    </view>
    <view id="viewAttachments1">
      <page>/pageFragments/masterHome/viewAttachments.jsff</page>
    </view>
    <view id="rejectionTitle1">
      <page>/pageFragments/masterHome/rejectionTitle.jsff</page>
    </view>
    <view id="viewAttachments2">
      <page>/pageFragments/masterHome/viewAttachments.jsff</page>
    </view>
    <view id="rejectionTitle2">
      <page>/pageFragments/masterHome/rejectionTitle.jsff</page>
    </view>
    <view id="viewAttachments3">
      <page>/pageFragments/masterHome/viewAttachments.jsff</page>
    </view>
    <view id="rejectionTitle3">
      <page>/pageFragments/masterHome/rejectionTitle.jsff</page>
    </view>
    <method-call id="fetchWKFVendorSiteRequest">
      <method>#{pageFlowScope.masterHomeBean.fetchWKFVendorSiteRequest}</method>
      <outcome>
        <to-string>true</to-string>
      </outcome>
    </method-call>
    <control-flow-rule id="__2">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>vendorResponses</from-outcome>
        <to-activity-id>vendorResponses</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__5">
        <from-outcome>pendingForCaApproval</from-outcome>
        <to-activity-id>foreignVendorPendingCAApproval</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__6">
        <from-outcome>vendorResponseToCaRjection</from-outcome>
        <to-activity-id>vendorResponsesToCARejection</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__9">
        <from-outcome>fetchWkfSiteRequests</from-outcome>
        <to-activity-id>fetchWKFVendorSiteRequest</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__10">
        <from-outcome>pendingRequests</from-outcome>
        <to-activity-id>pendingRequests</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__8">
      <from-activity-id>vendorResponses</from-activity-id>
      <control-flow-case id="__14">
        <from-outcome>partnerResponse</from-outcome>
        <to-activity-id>vendorResponseDetail</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__29">
        <from-outcome>viewAttachment</from-outcome>
        <to-activity-id>viewAttachments1</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__32">
        <from-outcome>reject</from-outcome>
        <to-activity-id>rejectionTitle1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__12">
      <from-activity-id>vendorResponsesToCARejection</from-activity-id>
      <control-flow-case id="__15">
        <from-outcome>partnerResponse</from-outcome>
        <to-activity-id>vendorResponseDetail1</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__42">
        <from-outcome>viewAttachment</from-outcome>
        <to-activity-id>viewAttachments3</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__46">
        <from-outcome>reject</from-outcome>
        <to-activity-id>rejectionTitle3</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__18">
      <from-activity-id>vendorResponseDetail1</from-activity-id>
      <control-flow-case id="__19">
        <from-outcome>close</from-outcome>
        <to-activity-id>vendorResponsesToCARejection</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__20">
      <from-activity-id>vendorResponseDetail</from-activity-id>
      <control-flow-case id="__21">
        <from-outcome>close</from-outcome>
        <to-activity-id>vendorResponses</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__22">
      <from-activity-id>pendingRequests</from-activity-id>
      <control-flow-case id="__23">
        <from-outcome>viewAttachment</from-outcome>
        <to-activity-id>viewAttachments</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__26">
        <from-outcome>reject</from-outcome>
        <to-activity-id>rejectionTitle</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__24">
      <from-activity-id>viewAttachments</from-activity-id>
      <control-flow-case id="__25">
        <from-outcome>close</from-outcome>
        <to-activity-id>pendingRequests</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__27">
      <from-activity-id>rejectionTitle</from-activity-id>
      <control-flow-case id="__28">
        <from-outcome>back</from-outcome>
        <to-activity-id>pendingRequests</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__30">
      <from-activity-id>viewAttachments1</from-activity-id>
      <control-flow-case id="__31">
        <from-outcome>close</from-outcome>
        <to-activity-id>vendorResponses</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__33">
      <from-activity-id>rejectionTitle1</from-activity-id>
      <control-flow-case id="__34">
        <from-outcome>back</from-outcome>
        <to-activity-id>vendorResponses</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__35">
      <from-activity-id>foreignVendorPendingCAApproval</from-activity-id>
      <control-flow-case id="__36">
        <from-outcome>viewAttachment</from-outcome>
        <to-activity-id>viewAttachments2</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__39">
        <from-outcome>reject</from-outcome>
        <to-activity-id>rejectionTitle2</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__37">
      <from-activity-id>viewAttachments2</from-activity-id>
      <control-flow-case id="__38">
        <from-outcome>close</from-outcome>
        <to-activity-id>foreignVendorPendingCAApproval</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__40">
      <from-activity-id>rejectionTitle2</from-activity-id>
      <control-flow-case id="__41">
        <from-outcome>back</from-outcome>
        <to-activity-id>foreignVendorPendingCAApproval</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__43">
      <from-activity-id>viewAttachments3</from-activity-id>
      <control-flow-case id="__44">
        <from-outcome>close</from-outcome>
        <to-activity-id>vendorResponsesToCARejection</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__47">
      <from-activity-id>rejectionTitle3</from-activity-id>
      <control-flow-case id="__48">
        <from-outcome>back</from-outcome>
        <to-activity-id>vendorResponsesToCARejection</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>fetchWKFVendorSiteRequest</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>goToPendingRequests</from-outcome>
        <to-activity-id>pendingRequests</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
