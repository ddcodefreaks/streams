<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
    <jsp:directive.page contentType="text/html;charset=UTF-8"/>
   <af:pageTemplateDef var="attrs" definition="private">
        <af:resource type="css" source="/css/fonts.css"/>
        <af:xmlContent>
            <component xmlns="http://xmlns.oracle.com/adf/faces/rich/component">
                <description/>
                <display-name>airtelDefaultPageTemplate</display-name>
                <facet>
                    <description>this is basic content area</description>
                    <facet-name>content</facet-name>
                </facet>
            </component>
        </af:xmlContent>
        <af:panelGroupLayout id="pt_pgl14" layout="vertical" styleClass="wrapper">
            <af:panelGroupLayout id="pt_pgl1" styleClass="mybody AFStretchWidth" layout="vertical"
                                 inlineStyle="float:right">
                <af:panelGroupLayout id="pt_pgl2" styleClass="navbar-default AFStretchWidth" layout="vertical">
                    <af:panelGroupLayout id="pt_pgl7" styleClass="container-fluid top-smal-menu AFStretchWidth"
