                        <af:panelGroupLayout id="pt_pgl3" styleClass="" layout="horizontal" inlineStyle="float:right;">
                            <af:link text="Careers" id="pt_l1" styleClass="topNavTab text-uppercase" 
                                            inlineStyle="padding: 3px;margin-left: 10px;"
                                     destination="https://www.airtel.in/careers/"/>
                            <af:link text="personal" id="pt_l2" styleClass="topNavTab text-uppercase"
                                            inlineStyle="padding: 3px;margin-left: 10px;"
                                     destination="https://www.airtel.in/"/>
                            <af:link text="business" id="pt_l3" styleClass="topNavTab text-uppercase"
                                            inlineStyle="padding: 3px;margin-left: 10px;"
                                     destination="https://www.airtel.in/business/"/>
                            <af:link text="investors" id="pt_l4" styleClass="topNavTab text-uppercase"
                                            inlineStyle="padding: 3px;margin-left: 10px;"
                                     destination="https://www.airtel.in/about-bharti/equity"/>
                        </af:panelGroupLayout>
                    </af:panelGroupLayout>
                    <af:panelGroupLayout id="pt_pgl4" styleClass="AFStretchWidth container-fluid large-menu"
