<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="partnerOrgDetailBTF">
    <default-activity>onLoadBtf</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__3">
      <managed-bean-name>partnerOrgDetailBean</managed-bean-name>
      <managed-bean-class>com.airtel.supplierportal.bean.partnerorgdetails.PartnerOrgDetails</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="partnerOrgDetails">
      <page>/pagefragments/partnerOrgDetail/partnerOrgDetails.jsff</page>
    </view>
    <view id="compnayInfoThanks">
      <page>/pagefragments/partnerOrgDetail/compnayInfoThanks.jsff</page>
    </view>
    <method-call id="onLoadBtf">
      <method>#{pageFlowScope.partnerOrgDetailBean.onLoadBtf}</method>
      <outcome>
        <to-string>true</to-string>
      </outcome>
    </method-call>
    <view id="nonDomesticThanks">
      <page>/pagefragments/partnerOrgDetail/nonDomesticThanks.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>partnerOrgDetails</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>submit</from-outcome>
        <to-activity-id>compnayInfoThanks</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__7">
        <from-outcome>nonDomesticSubmit</from-outcome>
        <to-activity-id>nonDomesticThanks</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>onLoadBtf</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>detailNotSubmited</from-outcome>
        <to-activity-id>partnerOrgDetails</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__6">
        <from-outcome>detailSubmited</from-outcome>
        <to-activity-id>compnayInfoThanks</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__8">
        <from-outcome>detailsSubmittedNonDomestic</from-outcome>
        <to-activity-id>nonDomesticThanks</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
