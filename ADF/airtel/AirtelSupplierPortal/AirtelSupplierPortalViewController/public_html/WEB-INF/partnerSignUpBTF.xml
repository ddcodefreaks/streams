<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="partnerSignUpBTF">
    <default-activity>partnerSignupView</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__5">
      <managed-bean-name>signupBean</managed-bean-name>
      <managed-bean-class>com.airtel.supplierportal.bean.signup.SignupBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="partnerSignupView">
      <page>/pagefragments/parnterSignup/partnerSignupView.jsff</page>
    </view>
    <view id="partnerSignup_2">
      <page>/pagefragments/parnterSignup/partnerSignup_2.jsff</page>
    </view>
    <view id="thankyou">
      <page>/pagefragments/parnterSignup/thankyou.jsff</page>
    </view>
    <view id="signupNoName">
      <page>/pagefragments/parnterSignup/signupNoName.jsff</page>
    </view>
    <view id="signupNoName_2">
      <page>/pagefragments/parnterSignup/signupNoName_2.jsff</page>
    </view>
    <view id="thankyou2">
      <page>/pagefragments/parnterSignup/thankyou2.jsff</page>
    </view>
    <view id="termsAndConditions">
      <page>/pagefragments/parnterSignup/termsAndConditions.jsff</page>
    </view>
    <method-call id="fetchProductServCat">
      <method>#{pageFlowScope.signupBean.loadProductsAndServices}</method>
      <outcome>
        <fixed-outcome>fetchProductServCat</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>partnerSignupView</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>continue</from-outcome>
        <to-activity-id>partnerSignup_2</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__11">
        <from-outcome>no</from-outcome>
        <to-activity-id>signupNoName</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>partnerSignup_2</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>createAccount</from-outcome>
        <to-activity-id>thankyou</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__6">
        <from-outcome>t&amp;cAccepted</from-outcome>
        <to-activity-id>termsAndConditions</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__17">
        <from-outcome>back</from-outcome>
        <to-activity-id>partnerSignupView</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>signupNoName</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>continue1</from-outcome>
        <to-activity-id>fetchProductServCat</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__12">
        <from-outcome>yes</from-outcome>
        <to-activity-id>partnerSignupView</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__9">
      <from-activity-id>signupNoName_2</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>submit</from-outcome>
        <to-activity-id>thankyou2</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__18">
        <from-outcome>back</from-outcome>
        <to-activity-id>signupNoName</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__13">
      <from-activity-id>termsAndConditions</from-activity-id>
      <control-flow-case id="__14">
        <from-outcome>onSigupPage</from-outcome>
        <to-activity-id>partnerSignup_2</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__15">
      <from-activity-id>fetchProductServCat</from-activity-id>
      <control-flow-case id="__16">
        <to-activity-id>signupNoName_2</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
