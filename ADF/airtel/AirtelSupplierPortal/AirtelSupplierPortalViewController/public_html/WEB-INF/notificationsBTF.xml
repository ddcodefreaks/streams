<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="notificationsBTF">
    <default-activity>onBTFLoad</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__3">
      <managed-bean-name>notifications</managed-bean-name>
      <managed-bean-class>com.airtel.supplierportal.bean.notifications.Notifications</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="notifications">
      <page>/pagefragments/notifications/notifications.jsff</page>
    </view>
    <view id="informationRequestsDomestic">
      <page>/pagefragments/notifications/informationRequestsDomestic.jsff</page>
    </view>
    <method-call id="onBTFLoad">
      <method>#{pageFlowScope.notifications.onBTFLoad}</method>
      <outcome>
        <fixed-outcome>onPageLoad</fixed-outcome>
      </outcome>
    </method-call>
    <task-flow-return id="onBackReturn">
      <outcome>
        <name>onBackReturn</name>
      </outcome>
    </task-flow-return>
    <view id="informationRequestNonDomestic">
      <page>/pagefragments/notifications/informationRequestNonDomestic.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>onBTFLoad</from-activity-id>
      <control-flow-case id="__2">
        <to-activity-id>notifications</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>notifications</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>back</from-outcome>
        <to-activity-id>onBackReturn</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__6">
        <from-outcome>informationRequests</from-outcome>
        <to-activity-id>informationRequestsDomestic</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>informationRequestsDomestic</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>notifications</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
