<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="addAnotherSiteAddressBTF">
    <default-activity>checkCountry</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>addAnotherSiteAddress</managed-bean-name>
      <managed-bean-class>com.airtel.supplierportal.bean.addAnotherSiteAddress.AddAnotherSiteAddress</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <method-call id="checkCountry">
      <method>#{pageFlowScope.addAnotherSiteAddress.checkCountry}</method>
      <outcome>
      <to-string>true</to-string>
      </outcome>
    </method-call>
    <view id="addAnotherSiteAddressDomestic">
      <page>/pagefragments/addAnotherSiteAddress/addAnotherSiteAddressDomestic.jsff</page>
    </view>
    <view id="addAnotherSiteAddressNonDomestic">
      <page>/pagefragments/addAnotherSiteAddress/addAnotherSiteAddressNonDomestic.jsff</page>
    </view>
    <view id="sitesAddThankyouDomestic">
      <page>/pagefragments/partnerRegistrationPhase2/sitesAddThankyouDomestic.jsff</page>
    </view>
    <view id="sitesAddThankyouNonDomestic">
      <page>/pagefragments/partnerRegistrationPhase2/sitesAddThankyouNonDomestic.jsff</page>
    </view>
    <task-flow-call id="partnerHomeBTF">
      <task-flow-reference>
        <document>/WEB-INF/partnerHomeBTF.xml</document>
        <id>partnerHomeBTF</id>
      </task-flow-reference>
    </task-flow-call>
    <control-flow-rule id="__2">
      <from-activity-id>checkCountry</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>domestic</from-outcome>
        <to-activity-id>addAnotherSiteAddressDomestic</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__4">
        <from-outcome>nonDomestic</from-outcome>
        <to-activity-id>addAnotherSiteAddressNonDomestic</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>addAnotherSiteAddressNonDomestic</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>partnerHomeBTF</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__8">
        <from-outcome>submit</from-outcome>
        <to-activity-id>sitesAddThankyouNonDomestic</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>addAnotherSiteAddressDomestic</from-activity-id>
      <control-flow-case id="__9">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>partnerHomeBTF</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__6">
        <from-outcome>submit</from-outcome>
        <to-activity-id>sitesAddThankyouDomestic</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__11">
      <from-activity-id>sitesAddThankyouNonDomestic</from-activity-id>
      <control-flow-case id="__12">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>partnerHomeBTF</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__16">
        <from-outcome>addAnotherSiteAddress</from-outcome>
        <to-activity-id>checkCountry</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__13">
      <from-activity-id>sitesAddThankyouDomestic</from-activity-id>
      <control-flow-case id="__14">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>partnerHomeBTF</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__15">
        <from-outcome>addAnotherSiteAddress</from-outcome>
        <to-activity-id>checkCountry</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
