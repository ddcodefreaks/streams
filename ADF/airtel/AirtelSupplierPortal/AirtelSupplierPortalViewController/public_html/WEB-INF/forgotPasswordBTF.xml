<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="forgotPasswordBTF">
    <default-activity>forgot</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>forgotpasswordBean</managed-bean-name>
      <managed-bean-class>com.airtel.supplierportal.bean.forgotpassword.ForgotPassword</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="forgot">
      <page>/pagefragments/forgotpassword/forgot.jsff</page>
    </view>
    <view id="otp">
      <page>/pagefragments/forgotpassword/otp.jsff</page>
    </view>
    <view id="SET-PASSWORD">
      <page>/pagefragments/forgotpassword/SET-PASSWORD.jsff</page>
    </view>
    <control-flow-rule id="__5">
      <from-activity-id>forgot</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>OTP</from-outcome>
        <to-activity-id>otp</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>otp</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>SETPWD</from-outcome>
        <to-activity-id>SET-PASSWORD</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
